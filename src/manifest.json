{
    "name": "CodeTime",
    "version": "1.0",
    "description": "Solve Leetcode questions and earn time to spend on your favorite sites!",
    "permissions": ["<all_urls>", "activeTab", "declarativeContent", "storage", "webNavigation", "webRequest"],
    "background": {
        "scripts": ["jquery-3.5.1.min.js", "scripts/localstorage.js", "scripts/leetcode.js", "scripts/background.js"],
        "persistent": true
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "icons": {
        "48": "codetime.png",
        "128": "codetime.png"
    },
    "manifest_version": 2
}